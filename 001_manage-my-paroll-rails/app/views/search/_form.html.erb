<%= search_form_for @q, url: employees_path, method: :get, class:'flex flex-row text-[15px] w-full bg-transparent focus:outline-none' do |f| %>
    <%= f.search_field :first_name_or_last_name_cont, class: 'form-control rounded-md' %>
    <%= f.submit 'Search', class:'text-white ml-1 font-semibold' %>
<% end %>
